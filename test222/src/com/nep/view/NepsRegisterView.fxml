<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="670.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nep.controller.NepsRegisterViewController"
            stylesheets="@../view/modern_style.css">
   <children>
      <Label  fx:id="titleLabel" layoutX="156.0" layoutY="54.0" text="环保公众监督平台">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label    fx:id="subtitleLabel" layoutX="216.0" layoutY="129.0" text="公众监督员注册" />
      <TextField fx:id="txt_id" layoutX="216.0" layoutY="199.0" />
      <PasswordField fx:id="txt_password" layoutX="216.0" layoutY="266.0" />
      <PasswordField fx:id="txt_repassword" layoutX="216.0" layoutY="335.0" />
      <TextField fx:id="txt_realName" layoutX="216.0" layoutY="399.0" />
      <Label layoutX="56.0" layoutY="207.0" text="注册手机号" />
      <Label layoutX="56.0" layoutY="274.0" text="密码" />
      <Label layoutX="56.0" layoutY="343.0" text="确认密码" />
