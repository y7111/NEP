            <ToggleGroup fx:id="gender" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="txt_sex" layoutX="326.0" layoutY="475.0" mnemonicParsing="false" text="女" toggleGroup="$gender" />
      <Button fx:id="back" layoutX="116.0" layoutY="545.0" mnemonicParsing="false" onAction="#back" text="返回" />
      <Button fx:id="register" layoutX="404.0" layoutY="545.0" mnemonicParsing="false" onAction="#register" text="注册" />
   </children>
</AnchorPane>
